    <nmo:Mint rdf:about="http://nomisma.org/id/montbeliard">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:prefLabel xml:lang="en">Montbéliard</skos:prefLabel>
        <skos:definition xml:lang="en">The mint(s) at the City of Montbéliard (Dep. Doubs) in France. As of 1033 the county of Montbéliard was a part of the Holy Roman Empire. From 1397 to 1796 (practically already 1793), under its German name Mömpelgard, it belonged to the house of Württemberg.</skos:definition>
        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/modern_european_numismatics"/>
        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/modern_german_numismatics"/>
        <skos:closeMatch rdf:resource="http://dbpedia.org/resource/Montbéliard"/>
        <skos:altLabel xml:lang="de">Mömpelgard</skos:altLabel>
        <skos:prefLabel xml:lang="fr">Montbéliard</skos:prefLabel>
        <geo:location rdf:resource="http://nomisma.org/id/montbeliard#this"/>
        <skos:altLabel xml:lang="de">Moempelgard</skos:altLabel>
        <skos:closeMatch rdf:resource="http://vocab.getty.edu/tgn/7009347"/>
        <skos:closeMatch rdf:resource="http://collection.britishmuseum.org/id/place/x29529"/>
        <skos:prefLabel xml:lang="de">Montbéliard</skos:prefLabel>
        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/medieval_european_numismatics"/>
    </nmo:Mint>
