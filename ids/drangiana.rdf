    <nmo:Region rdf:about="http://nomisma.org/id/drangiana">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:prefLabel xml:lang="en">Drangiana</skos:prefLabel>
        <skos:definition xml:lang="en">Drangiana was a historical region and administrative division of the Achaemenid Empire. This region comprises territory around Hamun Lake, wetlands in endorheic Sistan Basin on the Iran-Afghan border, and its primary watershed Helmand river in what is nowadays southwestern region of Afghanistan.</skos:definition>
        <skos:altLabel xml:lang="en">Zarangiana</skos:altLabel>
        <skos:altLabel xml:lang="en">Zranka</skos:altLabel>
