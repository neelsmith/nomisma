   <foaf:Person rdf:about="http://nomisma.org/id/octavian">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:prefLabel xml:lang="en">Octavian</skos:prefLabel>
        <skos:definition xml:lang="en">Adopted son of Julius Caesar, the future Emperor Augustus. The preferred URI is http://nomisma.org/id/augustus but this URI is available when specificity is necessary.</skos:definition>
