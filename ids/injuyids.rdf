  <nmo:hasEndDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1353</nmo:hasEndDate>
  <dcterms:isPartOf rdf:resource="http://nomisma.org/id/islamic_numismatics"/>
 </rdac:Family>
