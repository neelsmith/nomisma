    <foaf:Person rdf:about="http://nomisma.org/id/heinrich_roth_dorsten">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:prefLabel xml:lang="en">Heinrich Roth</skos:prefLabel>
        <skos:definition xml:lang="en">Roth was responsible for the production of low quality gelders in Dorsten (where Roth was known as Kühte), said to have been struck in the cellar of his house. In 1662, at the request of the Prince-Elector, Roth struck small coin denominations in Recklinghausen, notwithstanding the protestations of the Mayor and Council as Recklinghausen already had its own Mintmaster (Peter Nießmann). Lit.: Alfred Noss, Die Münzen der Erzbischöfe von Köln 1547-1794 (1925) pp. 245, 246, 247.</skos:definition>
        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/modern_german_numismatics"/>
        <org:hasMembership rdf:resource="http://nomisma.org/id/heinrich_roth_dorsten#modern_period_mint_staff"/>
        <org:hasMembership rdf:resource="http://nomisma.org/id/heinrich_roth_dorsten#modern_period_mint_staff2"/>
        <skos:altLabel xml:lang="de">Heinrich Kühte</skos:altLabel>
        <skos:altLabel xml:lang="de">Heinrich Roth gen. Kühte</skos:altLabel>
    </foaf:Person>
